<% include ../../partials/dashboard_header %>

<main id="users" role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
        <div class=" " style="margin-top: 20px; width: 100%;">
            <ul class="nav nav-pills nav-fill">
              <li class="nav-item">
                <a class="nav-link" href="/dashboard-users-add"><i style="margin-right: 15px;" class="fas fa-user-plus"></i>Add User</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/dashboard-users-edit"><i style="margin-right: 15px;" class="fas fa-user-edit"></i>Edit/Modify User</a>
              </li>
              <li class="nav-item">
                <a class="nav-link cus-active3" href="/dashboard-users-view"><i style="margin-right: 15px;" class="fas fa-list-ul"></i>View All Users</a>
              </li>
            </ul>
        </div>
        <div class="container" style="margin: 20px auto;">
          <table class="table table-striped">
            <thead>
              <th>Full Name</th>
              <th>Role</th>
              <th>Email</th>
              <th>Contact</th>
              <th>Institute</th>
              <th>Status</th>
            </thead>
            <% result.forEach(u=>{ %>
            <tr>
              <td><%= u.NAME %></td>
              <td><%= role[u.ROLE] %></td>
              <td><%= u.email %></td>
              <td><%= u.contact %></td>
              <td><%= ins[u.institute_id-1] %></td>
              <td><%= status[u.STATUS] %></td>
            </tr>
            <% })%>
            
          </table>
        </div>
      </main>
    </div>

<% include ../../partials/dashboard_footer %>
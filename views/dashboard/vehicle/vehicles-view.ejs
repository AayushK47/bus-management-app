<% include ../../partials/dashboard_header %>

<main id="users" role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
        <div class=" " style="margin-top: 20px; width: 100%;">
            <ul class="nav nav-pills nav-fill">
              <li class="nav-item">
                <a class="nav-link" href="/dashboard-vehicles-add"><i style="margin-right: 15px;" class="fas fa-car"></i>Add Vehicle</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/dashboard-vehicles-edit"><i style="margin-right: 15px;" class="fas fa-edit"></i>Edit/Modify Vehicle</a>
              </li>
              <li class="nav-item">
                <a class="nav-link cus-active3" href="/dashboard-vehicles-view"><i style="margin-right: 15px;" class="fas fa-list-ul"></i>View All Vehicles</a>
              </li>
            </ul>
        </div>
        <div class="container" style="margin: 20px auto;">
          <table class="table table-striped">
              <thead>
                  <th>Vehicle Registration No.</th>
                  <th>Route No.</th>
                  <th>Vehicle Type</th>
                  <th>Capacity</th>
                  <th>Institute</th>
              </thead>
              <% result.forEach(x =>{ %>
              <tr>
                <td><%= x.reg_no %></td>
                <td><%= x.route_no %></td>
                <td><%= vtype[x.v_type] %></td>
                <td><%= x.capacity %></td>
                <td><%= ins[x.institute_id-1] %></td>
              </tr>
              <% }) %>
          </table>
      </main>
    </div>

<% include ../../partials/dashboard_footer %>